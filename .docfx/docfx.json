{
  "metadata": [
    {
      "force": false,
      "src": [
        {
          "src": "../../../",
          "files": [
            "GDX.csproj",
            "GDX.Editor.csproj"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "filter": "filterConfig.yml" ,
      "properties": {
        "TargetFramework": "netstandard2.1",
        "DefineConstants": "UNITY_EDITOR;GDX_ADDRESSABLES;GDX_PLATFORMS;GDX_VISUALSCRIPTING;GDX_AI",
        "ResolveNugetPackages": false
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "manual/**.md",
          "manual/**/toc.yml",
          "reports/**.md",
          "reports/**.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "reports/coverage/**",
          "reports/**.dgml",
          "reports/**.xml",
          "images/**",
          "logo.svg",
          "favicon.ico",
          "styles/*.css",
          "styles/*.js"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_enableSearch": true,
      "_appTitle": "GDX",
      "_gitContribute": {
        "repo": "https://github.com/dotBunny/GDX",
        "branch": "main",
        "apiSpecFolder": ".docfx/apispec"
      }
    },
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/gdx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "sitemap":{
      "baseUrl": "https://gdx.dotbunny.com/",
      "priority": 0.1,
      "changefreq": "monthly"
     }
  }
}